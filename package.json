{
  "name": "vue-ci",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:css": "stylelint **/*.{css,scss,vue} --ignore-path .gitignore",
    "lint": "run-s lint:*",
    "fix": "run-s \"lint:* --fix\"",
    "format": "prettier . --write --ignore-path .gitignore",
    "update:deps": "pnpm update --latest --interactive",
    "preinstall": "node ./scripts/preinstall.js",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@vueuse/core": "^9.9.0",
    "animate.css": "^4.1.1",
    "axios": "^1.2.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify/json": "^2.1.157",
    "@minko-fe/postcss-pxtoviewport": "^1.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.57.1",
    "stylelint": "^14.16.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.3",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^1.0.18"
  }
}
